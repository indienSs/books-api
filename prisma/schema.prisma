generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model books {
  id               Int       @id @default(autoincrement())
  title            String?
  author           String?
  publication_date DateTime? @db.Date
  genres           String[]
}

model users {
  id            Int     @id @default(autoincrement())
  user_name     String?
  user_role     Int?
  password_hash String?
}
